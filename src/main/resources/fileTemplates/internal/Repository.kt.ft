package ${packageName}

import kotlinx.coroutines.flow.Flow

interface ${className} {
    suspend fun get${methodName}(id: Long): ${returnType}
    suspend fun update${methodName}(${paramName}: ${returnType})
    suspend fun delete${methodName}(${paramName}: ${returnType})
    suspend fun add${methodName}(${paramName}: ${returnType})
    fun all(): Flow<List<${returnType}>>
}
